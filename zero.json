{
    "agents": {
        "defaults": {
            "model": {
                "primary": "ollama/llama3.2:latest"
            },
            "thinkingDefault": "medium"
        }
    },
    "gateway": {
        "mode": "local",
        "bind": "lan",
        "auth": {
            "mode": "token",
            "token": "${ZERO_GATEWAY_TOKEN}"
        },
        "port": 19001,
        "http": {
            "endpoints": {
                "chatCompletions": {
                    "enabled": true
                }
            }
        }
    }
}